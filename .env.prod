# Production Environment Variables
NODE_ENV=prod
PORT=3000

# Database Configuration
POSTGRES_HOST=prod-database-host
POSTGRES_PORT=5432
POSTGRES_USER=prod_user
POSTGRES_PASSWORD=prod_password_change_me
POSTGRES_DB=nestjs_prod

# Redis Configuration
REDIS_HOST=prod-redis-host
REDIS_PORT=6379
REDIS_PASSWORD=prod_redis_password

# JWT Configuration
JWT_SECRET=prod-secret-key-change-in-production
JWT_EXPIRATION=12h

# API Configuration
API_PREFIX=api
API_VERSION=v1

# Logging
LOG_LEVEL=warn
