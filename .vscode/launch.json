{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Dev",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:debug"],
      "env": {
        "NODE_ENV": "dev"
      },
      "envFile": "${workspaceFolder}/.env.dev",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "restart": true,
      "outputCapture": "std",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "QA",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:debug"],
      "env": {
        "NODE_ENV": "qa"
      },
      "envFile": "${workspaceFolder}/.env.qa",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "restart": true,
      "outputCapture": "std",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Prod",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:debug"],
      "env": {
        "NODE_ENV": "prod"
      },
      "envFile": "${workspaceFolder}/.env.prod",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "restart": true,
      "outputCapture": "std",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
